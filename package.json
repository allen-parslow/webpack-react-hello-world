{
  "name": "webpack-react-hello-world",
  "version": "1.0.0",
  "description": "Hello Word for Webpack + React (with Docker)",
  "author": "Allen Parslow",
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "https://github.com/allen-parslow/webpack-hello-world"
  },
  "keywords": [
    "webpack",
    "hello",
    "word",
    "docker",
    "react"
  ],
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "clean": "[ -d 'build/' ] && rm -r build/ || :",
    "test": "jest",
    "publish-aws": "./aws/codedeploy-publish.sh allen-parslow-code",
    "create-aws": "./aws/create-env.sh"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.1.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.0",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.0",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "sass-lint": "^1.11.1",
    "sass-loader": "^6.0.6",
    "sasslint-webpack-plugin": "^1.0.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "react": "^15.6.2",
    "react-dom": "^15.6.2"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/enzyme/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/scripts/mockStyle.js"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/test-setup.js"
  }
}
